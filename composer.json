{
    "name":        "arcanesoft/backups",
    "description": "Backups module for ARCANESOFT.",
    "homepage":    "https://github.com/ARCANESOFT/Backups",
    "keywords":    ["arcanedev", "arcanesoft", "database", "backups"],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                   ">=7.0",
        "arcanesoft/auth":       "~2.2",
        "arcanesoft/core":       "~2.4",
        "spatie/laravel-backup": "~4.0"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Backups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Backups\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.4.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
